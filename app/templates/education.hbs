<div class="jumbo">
  <h2>Education</h2>
  <p>Some of the schooling I've done over the years.</p>
</div>

{{#each model as |education|}}
<div class="col-md-6 ">
  <article class="listing">
      <img src="{{education.image}}" alt="">
      <div class="detail-container">
        <h3> {{education.school}}</h3>
        <div class="detail owner">
          <span>Degree:</span> {{education.degree}}
        </div>
        <div class="detail type">
          <span>Location:</span> {{education.location}}
        </div>
        <div class="detail location">
          <span>Graduation Date:</span> {{education.graduated}}
        </div>
      </div>
  </article>

  <!-- {{location-map location=experience.location}} -->
  </div>
{{/each}}
